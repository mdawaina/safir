<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.safir" version="1.2.31" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Safer</name>
    <description>
        The Best way to travel arround the Kingdom.
    </description>
    <author email="mohameddawaina@gmail.com.com" href="http://mdawaina.com">
        Mohamed Dawaina
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon density="mdpi" height="192" platform="ios" src="res/ios/icon.png" width="192" />
    <icon height="180" platform="ios" src="res/ios/icon-60@3x.png" width="180" />
    <icon height="60" platform="ios" src="res/ios/icon-60.png" width="60" />
    <icon height="120" platform="ios" src="res/ios/icon-60@2x.png" width="120" />
    <splash height="1334" platform="ios" src="res/ios/Default-667h.png" width="750" />
    <splash height="2208" platform="ios" src="res/ios/Default-736h.png" width="1242" />
    <splash height="1242" platform="ios" src="res/ios/Default-Landscape-736h.png" width="2208" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="http://safir.mdawaina.com" />
    <plugin name="cordova-plugin-camera" spec="~2.4.1">
        <param name="CAMERA_USAGE_DESCRIPTION" value="We are using the Camera for something to allow the user to take a photo of his car" />
        <param name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="We are using the Photo Library to allow the user uploads his car " />
    </plugin>
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.6.3" />
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.2.1" />
    <engine name="android" spec="~6.1.2" />
</widget>
